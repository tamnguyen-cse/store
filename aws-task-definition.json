{
  "family": "store-to-fargate",
  "taskRoleArn": "arn:aws:iam::586422557502:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::586422557502:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "store-container",
      "image": "586422557502.dkr.ecr.ap-southeast-1.amazonaws.com/store",
      "cpu": 512,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "APP",
          "value": "store"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/store-to-fargate",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "networkMode": "awsvpc",
  "proxyConfiguration": {
    "type": "APPMESH",
    "containerName": "store-container",
    "properties": [
      {
        "name": "IgnoredUID",
        "value": "1337"
      },
      {
        "name": "ProxyIngressPort",
        "value": "15000"
      },
      {
        "name": "ProxyEgressPort",
        "value": "15001"
      },
      {
        "name": "AppPorts",
        "value": "8080"
      },
      {
        "name": "EgressIgnoredIPs",
        "value": ""
      }
    ]
  },
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
